{
  "name": "galevate-backend",
  "version": "0.0.0",
  "description": "",
  "main": "build/index.js",
  "type": "module",
  "scripts": {
    "start": "node --env-file=.env build/index.js",
    "clean:build": "node -e \"require('fs').rmSync('build', { recursive: true, force: true })\"",
    "build": "pnpm run clean:build && tsc",
    "dev": "tsc && pnpm run \"/dev:/\"",
    "dev:tsc": "tsc --watch --preserveWatchOutput",
    "dev:node": "node --enable-source-maps --watch --env-file=.env build/index.js",
    "lint": "eslint",
    "format": "prettier --check . --write"
  },
  "keywords": [],
  "author": "Jondahl \"Gamix\" Rhenius",
  "license": "ISC",
  "packageManager": "pnpm@10.11.0",
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@total-typescript/ts-reset": "^0.6.1",
    "fastify": "^5.4.0",
    "fastify-type-provider-zod": "^5.0.3",
    "moment": "^2.30.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@total-typescript/tsconfig": "^1.0.4",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^24.2.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0"
  }
